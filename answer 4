list_of_dictionary = [{'make': 'Nokia', 'model': 216, 'color': 'Black'},
                      {'make': 'Mi Max', 'model': '2', 'color': 'Gold'},
                      {'make': 'Samsung', 'model': 7, 'color': 'Blue'}]

''' for the desired output dictionary is sorted by color key'''
new = sorted(list_of_dictionary, key= lambda i : (i["color"]))
print(new)
